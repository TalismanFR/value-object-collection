{
  "name": "services/value-objects-collection",
  "description": "Collection of PHP value objects to use and extend as building blocks for DDD.",
  "type": "library",
  "keywords": [
    "valueobject",
    "DDD"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Anatoliy Egorov",
      "email": "a.egorov@briskly.online",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "giggsey/libphonenumber-for-php": "^8.0@dev",
    "egulias/email-validator": "^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "services\\valueobjects\\": "src"
    }
  },
  "scripts": {
    "phpcs": "phpcs --standard=PSR2"
  }
}
